	<!-- 生成XML -->
	<select id="getDeviceBySpace_ft" resultMap="DeviceResultMap"
		parameterType="Object">
		select a.DeviceID ,
		a.DeviceName ,
		a.DeviceType ,
		b.SampleTime as StartOperateTime,
		b.Remark
		from pubdevice a,
		sf6_data b
		where a.DeviceID=b.DeviceID
		and ( a.Space='低端换流变' or a.Space='高端换流变' or
		a.Space='站用变')
		UNION
		select a.DeviceID ,
		a.DeviceName ,
		a.DeviceType ,
		b.SampleTime as StartOperateTime,
		b.Remark
		from pubdevice a,
		scom_data b
		where a.DeviceID=b.DeviceID
		and ( a.Space='低端换流变' or a.Space='高端换流变' or
		a.Space='站用变')
		UNION
		select a.DeviceID ,
		a.DeviceName ,
		a.DeviceType ,
		b.SampleTime as StartOperateTime,
		b.Remark
		from pubdevice a,
		smoam_data b
		where a.DeviceID=b.DeviceID
		and ( a.Space='低端换流变' or a.Space='高端换流变' or
		a.Space='站用变')
		UNION
		select a.DeviceID ,
		a.DeviceName ,
		a.DeviceType ,
		b.SampleTime as StartOperateTime,
		b.Remark
		from pubdevice a,
		spdm_data b
		where a.DeviceID=b.DeviceID
		and a.Space != '微气象'
		and ( a.Space='低端换流变'
		or a.Space='高端换流变' or a.Space='站用变')
		UNION
		select a.DeviceID ,
		a.DeviceName ,
		a.DeviceType ,
		b.SampleTime as StartOperateTime,
		b.Remark
		from pubdevice a,
		stom_data b
		where a.DeviceID=b.DeviceID
		and (
		a.Space='低端换流变' or a.Space='高端换流变' or a.Space='站用变');
	</select>
	<select id="getDeviceBySpace" resultMap="DeviceResultMap"
		parameterType="Object">
		select a.DeviceID ,
		a.DeviceName ,
		a.DeviceType ,
		b.SampleTime as
		StartOperateTime,
		b.Remark
		from pubdevice a,
		sf6_data b
		where
		a.DeviceID=b.DeviceID
		and a.Space=#{Space}
		UNION
		select a.DeviceID ,
		a.DeviceName ,
		a.DeviceType ,
		b.SampleTime as StartOperateTime,
		b.Remark
		from pubdevice a,
		scom_data b
		where a.DeviceID=b.DeviceID
		and
		a.Space=#{Space}
		UNION
		select a.DeviceID ,
		a.DeviceName ,
		a.DeviceType ,
		b.SampleTime as StartOperateTime,
		b.Remark
		from pubdevice a,
		smoam_data b
		where a.DeviceID=b.DeviceID
		and a.Space=#{Space}
		UNION
		select a.DeviceID
		,
		a.DeviceName ,
		a.DeviceType ,
		b.SampleTime as StartOperateTime,
		b.Remark
		from pubdevice a,
		spdm_data b
		where a.DeviceID=b.DeviceID
		and
		a.Space != '微气象'
		and a.Space=#{Space}
		UNION
		select a.DeviceID ,
		a.DeviceName ,
		a.DeviceType ,
		b.SampleTime as StartOperateTime,
		b.Remark
		from pubdevice a,
		stom_data b
		where a.DeviceID=b.DeviceID
		and
		a.Space=#{Space};
		<!-- select DeviceID,DeviceName,DeviceType,time from pubdevice where Space=#{Space} -->
	</select>

